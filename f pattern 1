#include <stdio.h>

int main()
{
    int i,j;
	for(i=6;i>=1;i--)
	{
	    for(j=1;j<=i;j++)
	    {
	    if(j%2==0)
	    printf("_");
	    else
	    printf("*");
	    if(i==5&&j==5)
	    printf("_");
	   }
	   printf("\n\n");
	}
	 

       for(i=2;i<=6;i++)
      {
    for(j=1;j<=i;j++)
    {
       
         if(j%2==0)
	    printf("_");
	    else
	    printf("*");
    }
	    printf("\n\n");
	    
    
      
      }

    return 0;
}
