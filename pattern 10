#include <stdio.h>

int main()
{
    int i,j;
    int m=2;
   int  n=2;
    
    for(i=1;i<=3;i++)
    {
        
        for(j=1;j<=m;j++)
        {
            printf("*");
        }
        
        m=m*2;
        printf("\n");
        for(j=1;j<=n;j++)
        {
            printf("*\n");
        }
        
        n=n*2;
        
    }

    return 0;
}
