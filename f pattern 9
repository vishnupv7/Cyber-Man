#include <stdio.h>

int main()
{
    int i,j;
    for(i=1;i<=4;i++)
    {
        if(i%2==0){
            printf("*\n*\n");
        }else{
            printf("*\n");
        }
        for(j=1;j<=i*4&&i<=3;j++)
        {
            printf("*");
        }
        printf("\n");
        
    }

    return 0;
}
